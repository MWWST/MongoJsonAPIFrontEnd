    <title>1955 API Frontend</title>
</head>
<body>
    <div class="container">
        <div class="row">
        	<div class="col-sm-9 col-sm-offset-2">
        	<form id="apiSearchForm">
        		<div class="form-group">
        			<label>Search People Born in 1955</label>
        			<input type="text" id="searchPeople" name="person" class="form-control"></input>
        		</div>
        		<button type="submit" class="btn btn-primary pull-right">Search</button>
			</form>
			<div id="results"></div>
			<div id="person"></div>
            </div>
        </div>
    </div>
</body>
<script type="text/javascript">
$(document).ready(function() {

$('#apiSearchForm').submit(function(event){
    event.preventDefault();
    var person= $('#searchPeople').val();
    console.log(person);
    $.ajax({
     // type: 'GET',
     url: 'http://localhost:8011/'+person,
     // data: id,
     success: function(response){
         console.log(response);
         var data = response;
         $('#results').html('<h3>Found '+data.length+' results</h3>');
         for (x in data) {

         	$('#person').append('<li>'+data[x].name+ " Record added: " + data[x].created_at+'</li>');

         }
       }

    })

})


})
</script>
